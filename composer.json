{
  "name": "innocode-digital/wp-deferred-loading",
  "version": "1.4.1",
  "type": "wordpress-muplugin",
  "license": "GPL-2.0+",
  "require": {
    "php": ">=7.4",
    "composer/installers": "~1.0"
  },
  "description": "Defer loading of JavaScript and CSS files.",
  "require-dev": {
    "phpunit/phpunit": "^12.2",
    "brain/monkey": "^2.6"
  },
  "autoload-dev": {
    "psr-4": {
      "Innocode\\WPDeferredLoading\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  },
  "scripts": {
    "tests": "@php ./vendor/bin/phpunit"
  }
}
